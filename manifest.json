{
  "manifest_version": 2,
  "name": "SU Rate My Prof extension",
  "description": "Easily find your Professor's rating while registering for classes.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "./assets/Seattle_redhawks_logo.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",


  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "permissions": [
    "contextMenus"
  ]
}